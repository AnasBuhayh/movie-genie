# Local Environment Configuration Template
# Copy this file to .env.local and customize as needed

# API Configuration
VITE_API_URL=http://localhost:5000/api

# App Configuration
VITE_APP_TITLE=Movie Genie - Local

# Optional: User tracking (for personalized recommendations)
VITE_DEFAULT_USER_ID=

# Optional: Feature flags
VITE_ENABLE_SEMANTIC_SEARCH=true
VITE_ENABLE_PERSONALIZED_RECS=true
VITE_ENABLE_USER_FEEDBACK=true

# Optional: Development helpers
VITE_LOG_LEVEL=info
VITE_ENABLE_DEVTOOLS=true